    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Demo/is_update_demo",
            "args": ["Update/U1/UpdMBK_TCC_DF=2_t=0,06s","<","Matrix/M1/matrixMBK_TCC_DF=2_t=0,06s"],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Activer l'impression en mode Pretty pour gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C/C++: gcc générer le fichier actif",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}